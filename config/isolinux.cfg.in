default linux
prompt 1
timeout 950
display boot.msg
F1 help.msg
F2 advanced.msg

label linux
    kernel vmlinuz
    append initrd=initrd.gz fastboot=yes ramdisk_size=32000 root=/dev/ram3 vga=788 automatic=method:cdrom

label linux2
    kernel vmlinuz
    append initrd=initrd.gz fastboot=yes ramdisk_size=32000 root=/dev/ram3 vga=785 automatic=method:cdrom

label linux3
    kernel vmlinuz
    append initrd=initrd.gz fastboot=yes ramdisk_size=32000 root=/dev/ram3 vga=791 automatic=method:cdrom

label linux4
    kernel vmlinuz
    append initrd=initrd.gz fastboot=yes ramdisk_size=32000 root=/dev/ram3 vga16 automatic=method:cdrom
